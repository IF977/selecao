<h1>Listando :: Processos Seletivos Abertos</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th>Edital</th>
      <th>Linhas de Pesquisa</th>
      <th colspan="3">Ações</th>
    </tr>
  </thead>

  <tbody>
    <% @processo_seletivos_abertos.each do |processo_seletivos_aberto| %>
      <tr>
        <td><%= processo_seletivos_aberto.descricao %></td>
        <td><%= processo_seletivos_aberto.edital.descricao %></td>
        <td><%= processo_seletivos_aberto.desc_linhas_pesquisa %></td>
        <td><%= link_to 'Finalizar', "/processo_seletivos/finalizar_processo_seletivo/" + processo_seletivos_aberto.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>