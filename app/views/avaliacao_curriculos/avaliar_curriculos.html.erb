<h3>Listando :: Avaliações</h3>

<table>
	<thead>
    <tr>
      <th>Candidato</th>
      <th>Processo Seletivo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @avaliacao_curriculos.each do |avaliacao_curriculo| %>
      <tr>
        <td><%= avaliacao_curriculo.inscricao.nome_candidato %></td>
        <td><%= avaliacao_curriculo.inscricao.descricao_proc_seletivo %></td>
        <td><%= link_to 'Show', avaliacao_curriculo %></td>
        <td><%= link_to 'Edit', edit_avaliacao_curriculo_path(avaliacao_curriculo) %></td>
      </tr>
    <% end %>
  </tbody>
</table>