<%= simple_form_for(@avaliacao_curriculo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user, label_method: :nome_usuario %>
    <%= f.association :inscricao, label_method: :nome_candidato %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Salvar", class: "my-button" %>
    <%= link_to 'Voltar', editals_path %>
  </div>
<% end %>
